{
    "compilerOptions": {
      "moduleResolution": "node" ,
      "lib": [
        "es2017",
        "dom"
      ],
      "allowJs": true,
      "target": "es2017",
      "module": "esnext",
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true,
      "noEmit": true ,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
  
      },
    "include": [
      "client"
    ],
  }